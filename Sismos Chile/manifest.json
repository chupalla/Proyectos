{
    "manifest_version": 2,
    "name": "Sismos Chile",
    "description": "Listado de ultimos sismos ocurridos en Chile",
    "version": "2.0",
    "browser_action": {
        "default_icon": "icono.png",
        "default_popup": "popup.html",
        "default_title": "Sismos Chile"
    },
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"contextMenus"
	],
	"icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    }
}