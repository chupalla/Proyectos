{
    "manifest_version": 2,
    "name": "Indicadores Economicos",
    "description": "Indicadores Economicos de Chile",
    "version": "2.0",
    "browser_action": {
        "default_icon": "img/icono.png",
        "default_popup": "popup.html",
        "default_title": "Indicadores Economicos"
    },
    "icons": { 
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png" 
    },
    "permissions": [
		"contextMenus"
	],
	"background": {
		"scripts": ["js/background.js"]
	}
}